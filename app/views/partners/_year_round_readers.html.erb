<%= form_for(@year_round_readers) do |f| %>
  <fieldset>
    <legend>Year Round Readers</legend>
  <% if @year_round_readers.errors.any? %>
    <div id="error_explanation">
      <h2><%= pluralize(@year_round_readers.errors.count, "error") %> prohibited this year_round_readers from being saved:</h2>

      <ul>
      <% @year_round_readers.errors.full_messages.each do |message| %>
        <li><%= message %></li>
