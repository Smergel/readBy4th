<html>
<head>
  <title>Partner Page</title>
</head>
<body>
<!-- <h1>Page That Partners See When They Log In</h1> -->
<p id="notice"><%= notice %></p>

<h1 class="alternate divide alternate-divide"></h1>

<div class="row">
  <div class="large-5 columns">
    <div class="vcard">
      <h1><%= @partner.name %></h1>
      <div class="logo"> <%= image_tag(@partner.logo.url(:thumb)) %> </div>
      <div class="email"><span class="vcard-label">Email: </span><a><%= @partner.email %></a></div>
      <div class="tel"><span class="type vcard-label">Phone: </span><a href="tel:+<% @partner.phone %>"><%= @partner.phone %></a></div>
      <div class="adr">
        <div class="email"><span class="vcard-label">Website: </span><a href="<% @partner.website %>"><%= @partner.website %></a></div>
        <span class="vcard-label">Address: </span>
        <div class="street-address"><%= @partner.address %></div>
        <span class="locality">Philadelphia</span>,
        <abbr class="region" title="Pennsylvania">PA</abbr>,
        <span class="postal-code"><%= @partner.zip_code %></span>
      </div>
      <%= link_to 'Edit', edit_partner_path(@partner) %>
    </div>
  </div>
