</div>
<div class="row">
  <div class="large-24 columns">
    <%= form_for Photo.new, html: {multipart: true} do |f| %>
    <%= f.label :upload_photo, class: 'photo-label' %>
    <%= f.file_field :photo %>
    <%= f.submit :upload %>
    <% end %>
  </div>
</div>
