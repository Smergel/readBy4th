<h1 class="alternate divide alternate-divide"></h1>
<div class="row">
  <div class="large-24 columns">
    <div class="photo-slider">
      <% if @photos %>
        <% @photos.each do |x|%>
          <div class="photo"><%= image_tag(x.photo) %></div>
        <% end %>
      <% else %>
      <div class="photo"><img src="https://libwww.freelibrary.org/images/kids/homework.jpg" alt=""></div>
