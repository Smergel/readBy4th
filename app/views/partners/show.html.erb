<h1>Page That Partners See When They Log In</h1>


<p id="notice"><%= notice %></p>

<p>
  <strong>Name:</strong>
  <%= @partner.name %>
</p>

<p>
  <strong>Address:</strong>
  <%= @partner.address %>
</p>

<p>
  <strong>Zip code:</strong>
  <%= @partner.zip_code %>
</p>

<p>
  <strong>Website:</strong>
  <%= @partner.website %>
</p>

<p>
  <strong>Email:</strong>
  <%= @partner.email %>
</p>

<p>
  <strong>Program start:</strong>
  <%= @partner.program_start %>
</p>

<p>
  <strong>Program end:</strong>
  <%= @partner.program_end %>
</p>



<p>
  <strong>Phone:</strong>
  <%= @partner.phone %>
</p>

<%= link_to 'Edit', edit_partner_path(@partner) %> |
<%= link_to 'Back', partners_path %><br />

<%= link_to "Add Participant", new_participant_path %><br />


<%= render "family_engagement" %><br />
<%= render "year_round_readers" %><br />

<ul>
<% @students.each do |x| %>
  <li>
    <%= x.first_name %>
    <%= form_for(@attendance_matter) do |f| %>
    <%= @participant_id = x.id %>
    <%= f.label :absences %>
    <%= f.number_field :absences %>
    <%= f.label :asthma_related %>
    <%= f.check_box :asthma %>
    <%= f.label :reason %>
    <%= f.text_field :reason %>
    <%= f.label :excused %>
    <%= f.check_box :excused %>
    <%= f.submit :Submit %>
    <% end %>
  </li>
<% end %>
</ul>















